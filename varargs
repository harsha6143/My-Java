class c1 {
    public int addition(int a, int b) {
        return a + b;

    }
    int sum = 0;
    public int addition(int... a) {
        for (int i : a) {
            sum += i;

        }
        return sum;
    }
}

class test
{
    public static void main(String[] args){
        c1 l=new c1();
        System.out.println(l.addition(1,2));
        System.out.println(l.addition(1,2,3));

    }
}
